    <string name="hint_supplier">sales@supplier.com</string>

    <!-- Text hint for quantity field in the editor [CHAR LIMIT=30] -->
    <string name="hint_item_quantity">Quantity</string>

    <!-- Units for price of the inventory ($) [CHAR LIMIT=5] -->
    <string name="unit_item_price">$</string>

